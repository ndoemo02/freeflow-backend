{
  "name": "freeflow-backend",
  "version": "1.4.2",
  "private": true,
  "type": "module",
  "main": "api/server-vercel.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node api/server-vercel.js",
    "vercel-build": "echo Using server-vercel.js handler",
    "postpatch": "node dev-tools/postpatch.cjs",
    "log": "node -e \"import('./dev-tools/patchLogger.js').then(m => m.logCursorPatch('Manual log', process.argv[1]))\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:performance": "vitest run tests/performance",
    "patch:nlu": "node scripts/amber-nlu-patcher.js",
    "test:logic": "vitest run tests/brain-logic-flow.test.js --reporter=verbose",
    "test:lastLocation": "vitest run tests/brain-cascade.test.js -t \"should remember last location\" --reporter=verbose",
    "test:critical": "vitest run tests/brain-cascade.test.js -t \"Tier 11\" --reporter=verbose",
    "test:fallback": "vitest run tests/brain-fallback.test.js --reporter=verbose",
    "test:summary": "node scripts/amber-test-summary.js",
    "test:parse": "node tests/test-parse.js",
    "test:brain": "node run-brain-tests.js",
    "test:orders": "npx vitest tests/orders-api.test.js --run"
  },
  "dependencies": {
    "@google-cloud/speech": "^6.0.0",
    "@google-cloud/text-to-speech": "^5.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.0",
    "vitest": "^1.0.0"
  }
}
